<h1>Edit Menu</h1>
<%= form_for [@restaurant, @menuitem] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :image %>
  <%= f.file_field :image %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.text_field :price %><br/>
  <%= f.submit %>
<% end %>