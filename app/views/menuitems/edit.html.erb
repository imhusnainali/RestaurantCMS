<div class="row">
  <div class="grid_4 offset_4">
    <h1>Edit Menu</h1>
    <%= form_for [@restaurant, @menuitem] do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %><br/>
      <%= f.label :image %>
      <%= f.file_field :image %><br/>
      <%= f.label :description %>
      <%= f.text_field :description %><br/>
      <%= f.label :price %>
      <%= f.text_field :price %><br/>
      <%= f.submit %>
    <% end %>
  </div>
</div>